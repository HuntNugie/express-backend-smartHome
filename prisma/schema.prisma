generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model smartHome {
  id Int @id @default(autoincrement())
  objek String 
  status Boolean
  pin Int 
  created_at DateTime @default(now())
}
